@{
    ViewBag.Title = "Thông báo lỗi";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<div id="page-wrapper" class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <h4 class="page-header form-title"><i class="fa fa-codepen"></i> @ViewBag.Title</h4>
        </div>
    </div>
    <!-- /.col-lg-12 -->
    <div class="row">
        <div class="col-lg-12">
            <img src="~/Content/Images/icon-error-404.png" style="margin-bottom:24px;" />
        </div>
        <div class="col-lg-12 text-danger">
            @ViewBag.ErrorMessage
        </div>
    </div>
</div>